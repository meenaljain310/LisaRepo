package Com.cg;


public class Employee {
	
	private int id;
	private String name;
	private String entity;
	private String department;

	public Employee(int id,String name,String entity,String department)
	{
		this.id= id;
		this.name= name;
		this.entity= entity;
		this.department= department;
	}
		
		
	}

